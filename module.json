{
  "id": "effectivetray-ng",
  "title": "Effective Tray NG",
  "description": "A module for dnd5e on foundryvtt allowing players to make full use of the effects and damage trays, instead of restricting them to DM only.",
  "version": "13.351-5.2.0-1",
  "compatibility": {
    "minimum": "13",
    "verified": "13.351",
    "maximum": "13.999"
  },
  "esmodules": [
    "scripts/libwrapper-shim/shim.js",
    "scripts/setup.mjs"
  ],
  "styles": [
    "styles/effectivetray.css"
  ],
  "relationships": {
    "systems": [
      {
        "id": "dnd5e",
        "type": "system",
        "compatibility": {
          "minimum": "5.2.0",
          "verified": "5.2.0",
          "maximum": "5.2.9999"
        }
      }
    ],
    "recommends": [
      {
        "id": "lib-wrapper",
        "type": "module",
        "compatibility": {
          "minimum": "1.13.0.0",
          "verified": "1.13.4.0"
        }
      }
    ]
  },
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json",
      "flags": {}
    }
  ],
  "flags": {
    "hotReload": {
      "extensions": [
        "css",
        "json"
      ],
      "paths": [
        "styles/effectivetray.css",
        "lang"
      ]
    }
  },
  "socket": true,
  "url": "https://github.com/alterNERDtive/effectivetray",
  "bugs": "https://github.com/alterNERDtive/effectivetray/issues/",
  "changelog": "https://github.com/alterNERDtive/effectivetray/releases/latest/",
  "download": "https://github.com/alterNERDtive/effectivetray/archive/refs/heads/main.zip",
  "manifest": "https://github.com/alterNERDtive/effectivetray/raw/refs/heads/main/module.json",
  "license": "LICENSE",
  "readme": "README.md",
  "authors": [
    {
      "name": "alterNERDtive",
      "discord": "alternerdtive",
      "url": "https://alternerd.tv",
      "flags": {}
    }
  ]
}
